#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/apple/Project/bpt-web/node_modules/.pnpm/semantic-release@23.0.8_typescript@4.9.5/node_modules/semantic-release/bin/node_modules:/Users/apple/Project/bpt-web/node_modules/.pnpm/semantic-release@23.0.8_typescript@4.9.5/node_modules/semantic-release/node_modules:/Users/apple/Project/bpt-web/node_modules/.pnpm/semantic-release@23.0.8_typescript@4.9.5/node_modules:/Users/apple/Project/bpt-web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/apple/Project/bpt-web/node_modules/.pnpm/semantic-release@23.0.8_typescript@4.9.5/node_modules/semantic-release/bin/node_modules:/Users/apple/Project/bpt-web/node_modules/.pnpm/semantic-release@23.0.8_typescript@4.9.5/node_modules/semantic-release/node_modules:/Users/apple/Project/bpt-web/node_modules/.pnpm/semantic-release@23.0.8_typescript@4.9.5/node_modules:/Users/apple/Project/bpt-web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/semantic-release@23.0.8_typescript@4.9.5/node_modules/semantic-release/bin/semantic-release.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/semantic-release@23.0.8_typescript@4.9.5/node_modules/semantic-release/bin/semantic-release.js" "$@"
fi
